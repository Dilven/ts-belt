{
  "name": "@mobily/ts-belt",
  "description": "ðŸ”§ TS Belt is a library for functional programming in TypeScript. It solves the problem of the existence of both `undefined` and `null`. TS Belt is inspired by the Belt module for ReScript/Reason.",
  "version": "1.0.6",
  "source": "./src/index.ts",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist/src",
    "*.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "build": "yarn test && yarn clean && yarn tsc -p tsconfig.build.json",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "generate:contributors": "yarn all-contributors generate"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "functional-programming",
    "fp",
    "monad",
    "option",
    "option-type",
    "result",
    "result-type",
    "array"
  ],
  "author": "Marcin Dziewulski <hello@mobily.pl>",
  "homepage": "https://github.com/mobily/ts-belt#readme",
  "bugs": "https://github.com/mobily/ts-belt/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobily/ts-belt.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "all-contributors-cli": "^6.20.0",
    "bs-platform": "^9.0.2",
    "coveralls": "^3.1.0",
    "esbuild": "^0.11.18",
    "flow-bin": "^0.150.0",
    "flowgen": "^1.14.1",
    "gentype": "^3.48.0",
    "globby": "^11.0.3",
    "jest": "^26.6.3",
    "jscodeshift": "^0.12.0",
    "minimatch": "^3.0.4",
    "prettier": "^2.2.0",
    "ts-jest": "^26.5.5",
    "tsd": "^0.14.0",
    "typescript": "^4.2.4",
    "wonka": "^4.0.15"
  },
  "cacheDirectories": [
    "node_modules"
  ]
