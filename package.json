{
  "name": "@mobily/ts-belt",
  "description": "ðŸ”§ TS Belt is a library for functional programming in TypeScript. It solves the problem of the existence of both `undefined` and `null`. TS Belt is inspired by the Belt module for ReScript/Reason.",
  "version": "1.0.6",
  "source": "./src/index.ts",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "src",
    "dist/src",
    "*.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf ./dist && rm -f ./tsconfig.tsbuildinfo",
    "build": "yarn test && yarn clean && yarn tsc -p tsconfig.build.json",
    "test": "yarn jest",
    "test:watch": "yarn jest --watch",
    "generate:contributors": "yarn all-contributors generate"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "functional-programming",
    "fp",
    "monad",
    "option",
    "option-type",
    "result",
    "result-type",
    "array"
  ],
  "author": "Marcin Dziewulski <hello@mobily.pl>",
  "homepage": "https://github.com/mobily/ts-belt#readme",
  "bugs": "https://github.com/mobily/ts-belt/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/mobily/ts-belt.git"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "all-contributors-cli": "^6.19.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.14.0",
    "import-sort-style": "^6.0.0",
    "import-sort-style-mobily": "^0.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "prettier-plugin-import-sort": "^0.0.6",
    "ts-jest": "^26.4.4",
    "tsd": "^0.13.1",
    "typescript": "^4.1.2"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
